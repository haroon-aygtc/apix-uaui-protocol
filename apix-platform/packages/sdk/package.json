{
  "name": "@apix/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for the APIX platform (REST + WebSocket)",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "socket.io-client": "^4.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}